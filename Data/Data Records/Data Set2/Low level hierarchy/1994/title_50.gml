    class_name "section"
    text "&#167;2204. Definitions"
  ]
  node [
    id 2533
    label "section_2204_item_1"
    class_name "item"
    text "(1) the term &#8220;appropriate congressional committees&#8221; means the Committee on Foreign Relations of the Senate and the Committee on Foreign Affairs of the House of Representatives, or, where required by law for certain reporting purposes, the Select Committee on Intelligence of the Senate and the Select&#160;1 Committee on Intelligence of the House of Representatives;"
  ]
  node [
    id 2534
    label "section_2204_item_2"
    class_name "item"
    text "(2) the term &#8220;enemy&#8221; means any country, government, group, or person that has been engaged in hostilities, whether or not lawfully authorized, with the United States;"
  ]
  node [
    id 2535
    label "section_2204_item_3"
    class_name "item"
    text "(3) the term &#8220;person&#8221; means&#8212;"
  ]
  node [
    id 2536
    label "section_2204_item_3_subitem_A"
    class_name "subitem"
    text "(A) any natural person;"
  ]
  node [
    id 2537
    label "section_2204_item_3_subitem_B"
    class_name "subitem"
    text "(B) any corporation, partnership, or other legal entity; and"
  ]
  node [
    id 2538
    label "section_2204_item_3_subitem_C"
    class_name "subitem"
    text "(C) any organization, association, or group; and"
  ]
  node [
    id 2539
    label "section_2204_item_4"
    class_name "item"
    text "(4) the term &#8220;spoils of war&#8221; means enemy movable property lawfully captured, seized, confiscated, or found which has become United States property in accordance with the laws of war."
  ]
  node [
    id 2540
    label "section_2205"
    class_name "section"
    text "&#167;2205. Construction"
  ]
  node [
    id 2541
    label "section_2205_item_1"
    class_name "item"
    text "(1) the abandonment or failure to take possession of spoils of war by troops in the field for valid military reasons related to the conduct of the immediate conflict, including the burden of transporting such property or a decision to allow allied forces to take immediate possession of certain property solely for use during an ongoing conflict;"
  ]
  node [
    id 2542
    label "section_2205_item_2"
    class_name "item"
    text "(2) the abandonment or return of any property obtained, borrowed, or requisitioned for temporary use during military operations without intent to retain possession of such property;"
  ]
  node [
    id 2543
    label "section_2205_item_3"
    class_name "item"
    text "(3) the destruction of spoils of war by troops in the field;"
  ]
  node [
    id 2544
    label "section_2205_item_4"
    class_name "item"
    text "(4) the return of spoils of war to previous owners from whom such property had been seized by enemy forces; or"
  ]
  node [
    id 2545
    label "section_2205_item_5"
    class_name "item"
    text "(5) minor articles of personal property which have lawfully become the property of individual members of the armed forces as war trophies pursuant to public written authorization from the Department of Defense."
  ]
