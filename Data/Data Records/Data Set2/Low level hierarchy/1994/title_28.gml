    class_name "section"
    text "&#167;653. Powers of arbitrator; arbitration hearing"
  ]
  node [
    id 1877
    label "section_653_subsection_a"
    class_name "subsection"
    text "(a) Powers.&#8212;An arbitrator to whom an action is referred under section 652 shall have, within the judicial district of the district court which referred the action to arbitration, the power&#8212;"
  ]
  node [
    id 1878
    label "section_653_subsection_a_item_1"
    class_name "item"
    text "(1) to conduct arbitration hearings,"
  ]
  node [
