  node [
    id 3204
    label "section_2402_item_1"
    class_name "item"
    text "Any action against the United States under section 1346 shall be tried by the court without a jury, except that any action against the United States under section 1346(a)(1) shall, at the request of either party to such action, be tried by the court with a jury."
  ]
  node [
