  ]
  node [
    id 1866
    label "section_652_subsection_b"
    class_name "subsection"
    text "(b) Actions That May Not Be Referred Without Consent of Parties.&#8212;Referral to arbitration under subsection (a)(1)(B) may not be made&#8212;"
  ]
  node [
    id 1867
    label "section_652_subsection_b_item_1"
    class_name "item"
    text "(1) of an action based on an alleged violation of a right secured by the Constitution of the United States, or"
  ]
  node [
